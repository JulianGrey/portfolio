{
  "name": "portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "install": "cd frontend && pnpm install --frozen-lockfile && cd ../backend && pnpm install",
    "install:frontend": "cd frontend && pnpm install --frozen-lockfile",
    "install:backend": "cd backend && pnpm install --frozen-lockfile",
    "dev": "cd frontend && pnpm run dev",
    "dev:backend": "cd backend && pnpm run local-server",
    "dev:db": "cd backend && pnpm run local-docker",
    "setup:backend": "cd backend && pnpm run setup-and-run-local",
    "build": "cd frontend && pnpm run build",
    "lint": "cd frontend && pnpm run lint",
    "preview": "cd frontend && pnpm run preview",
    "test": "cd frontend && pnpm run test"
  }
}
